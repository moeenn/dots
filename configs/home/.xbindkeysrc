"tilix"
  Mod4 + t

"thunar"
  Mod4 + e

"run"
  alt + p

"dmenu_startmenu"
  F1

"i3lock -c 000000"
  Mod4 + l

"scrot"
  Print

"pactl set-sink-volume @DEFAULT_SINK@ +3000"
   XF86AudioRaiseVolume

"pactl set-sink-volume @DEFAULT_SINK@ -3000"
   XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute

"playerctl play-pause"
  XF86AudioPlay

"playerctl play-pause"
  XF86AudioStop

"playerctl previous"
  XF86AudioPrev

"playerctl next"
  XF86AudioNext
