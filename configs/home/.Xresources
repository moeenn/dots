*font: xft:Fira Code:style=Regular:size=9
*allowBoldFonts: false
*.scaleHeight: 1.3

!! XTerm settings. 
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
xterm*geometry: 100x20
xterm*selectToClipboard: true
XTerm*internalBorder: 4

*.background:  #f9f5d7
*.foreground:  #674937
*.cursorColor: #674937

! black
*.color0:      #654735
*.color8:      #654735

! red
*.color1:      #c14a4a
*.color9:      #c14a4a

! green
*.color2:      #6c782e
*.color10:     #6c782e

! yellow
*.color3:      #b47109
*.color11:     #b47109

! blue
*.color4:      #45707a
*.color12:     #45707a

! magenta
*.color5:      #945e80
*.color13:     #945e80

! cyan
*.color6:      #4c7a5d
*.color14:     #4c7a5d

! white
*.color7:      #eee0b7
*.color15:     #eee0b7
