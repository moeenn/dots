*font: xft:JetBrains Mono:style=Regular:size=10
*allowBoldFonts: false
*.scaleHeight: 1.3

!! XTerm settings. 
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
xterm*geometry: 100x20
xterm*selectToClipboard: true
XTerm*internalBorder: 4

*.background:  #000000
*.foreground:  #d8d8d8
*.cursorColor: #d8d8d8

! black
*.color0:      #000000
*.color8:      #777777

! red
*.color1:      #ac4242
*.color9:      #ac4242

! green
*.color2:      #90a959
*.color10:     #90a959

! yellow
*.color3:      #f4bf75
*.color11:     #f4bf75

! blue
*.color4:      #6a9fb5
*.color12:     #6a9fb5

! magenta
*.color5:      #aa759f
*.color13:     #aa759f

! cyan
*.color6:      #75b5aa
*.color14:     #75b5aa

! white
*.color7:      #d8d8d8
*.color15:     #d8d8d8
