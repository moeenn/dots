"thunar"
    Mod4 + e
    

"xfce4-terminal"
    Mod4 + t
    
    
"pavucontrol"
    Mod4 + v
  
 
"rofi -show run -font 'Cascadia Code 11' -theme Monokai"
    Mod1 + p 
    
    
"i3lock -c 000000"
    Mod4 + l


"gnome-screenshot -i"
    Print
    

"xbacklight -inc 5"
    XF86MonBrightnessUp
    
    
"xbacklight -dec 5"
    XF86MonBrightnessDown
    

"amixer -q sset Master 3%+"
    XF86AudioRaiseVolume
    
    
"amixer -q sset Master 3%-"
    XF86AudioLowerVolume
    

"amixer -q set Master toggle"
    XF86AudioMute
    
    
"amixer -q set Capture toggle"
    XF86AudioMicMute
    
    
